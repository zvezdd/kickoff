{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2015"],  
    "jsx": "react-jsx",        
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
